[composite:gcenter]
use = egg:Paste#urlmap
/v1.0: public

[filter:auth]
paste.filter_factory = goperation.manager.filters.filter:AuthFilter.factory

[filter:cors]
paste.filter_factory = goperation.manager.filters.filter:CorsFilter.factory

[app:centerapi]
paste.app_factory = goperation.api.route:app_factory

[pipeline:public]
pipeline = cors auth centerapi